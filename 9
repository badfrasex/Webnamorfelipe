pan> gostaria de come\u00e7ar?'\r\n            };\r\n\r\n            \/\/ Fun\u00e7\u00e3o para avan\u00e7ar automaticamente\r\n            function autoAdvance() {\r\n                setTimeout(function() {\r\n                    if (currentStep < totalSteps) {\r\n                        currentStep++;\r\n                        updateUI();\r\n                    } else {\r\n                        \/\/ Quiz finalizado - redirecionar\r\n                        window.location.href = \"https:\/\/namor-ia.com\/generate-ai\";\r\n                    }\r\n                }, 800); \/\/ Delay de 800ms para dar feedback visual\r\n            }\r\n\r\n            \/\/ Fun\u00e7\u00e3o para atualizar a interface\r\n            function updateUI() {\r\n                \/\/ Atualizar barra de progresso\r\n                const progress = (currentStep \/ totalSteps) * 100;\r\n                progressBar.style.setProperty('--progress', progress + '%');\r\n\r\n                \/\/ Atualizar t\u00edtulo\r\n                quizTitle.innerHTML = stepTitles[currentStep];\r\n\r\n                \/\/ Mostrar\/esconder etapas\r\n                steps.forEach(function(step, index) {\r\n                    if (index + 1 === currentStep) {\r\n                        step.classList.add('active');\r\n                    } else {\r\n                        step.classList.remove('active');\r\n                    }\r\n                });\r\n\r\n                \/\/ Controlar bot\u00f5es\r\n                if (currentStep === 1) {\r\n                    btnBack.style.visibility = 'hidden';\r\n                } else {\r\n                    btnBack.style.visibility = 'visible';\r\n                }\r\n                btnNext.disabled = true;\r\n\r\n                \/\/ Parar qualquer \u00e1udio tocando ao mudar de etapa\r\n                if (currentAudio) {\r\n                    currentAudio.pause();\r\n                    currentAudio.currentTime = 0;\r\n                }\r\n\r\n                \/\/ Parar v\u00eddeo ao sair da etapa 17\r\n                if (currentStep !== 17 && videoPlayer) {\r\n                    videoPlayer.pause();\r\n                    videoPlayer.currentTime = 0;\r\n                }\r\n\r\n                \/\/ Autoplay e mute para v\u00eddeo na etapa 17\r\n                if (currentStep === 17 && videoPlayer) {\r\n                    videoPlayer.muted = true;\r\n                    videoPlayer.play().catch(function(error) {\r\n                        console.log(\"Autoplay bloqueado: \", error);\r\n                    });\r\n                }\r\n\r\n                \/\/ Para a etapa 14 (sliders), habilitar pr\u00f3ximo automaticamente\r\n                if (currentStep === 14) {\r\n                    btnNext.disabled = false;\r\n                }\r\n            }\r\n\r\n            \/\/ Fun\u00e7\u00e3o para adicionar eventos aos cards com avan\u00e7o autom\u00e1tico\r\n            function addCardEvents(stepId, dataAttribute) {\r\n                const cards = document.querySelectorAll('#' + stepId + ' .card');\r\n                cards.forEach(function(card) {\r\n                    card.addEventListener(\"click\", function() {\r\n                        if (!card.classList.contains('locked')) {\r\n                            cards.forEach(function(c) {\r\n                                c.classList.remove(\"selected\");\r\n                            });\r\n                            card.classList.add(\"selected\");\r\n                            quizData[dataAttribute] = card.dataset[dataAttribute];\r\n                            autoAdvance();\r\n                        }\r\n                    });\r\n                });\r\n            }\r\n\r\n            \/\/ Adicionar eventos para todas as etapas com cards\r\n            addCardEvents('step-1', 'style');\r\n            addCardEvents('step-3', 'age');\r\n            addCardEvents('step-4', 'body');\r\n            addCardEvents('step-5', 'breast');\r\n            addCardEvents('step-6', 'butt');\r\n            addCardEvents('step-7', 'ethnicity');\r\n            addCardEvents('step-8', 'eyecolor');\r\n            addCardEvents('step-9', 'hairstyle');\r\n            addCardEvents('step-10', 'haircolor');\r\n            addCardEvents('step-11', 'feature');\r\n            addCardEvents('step-18', 'clothes');\r\n\r\n            \/\/ Etapa 2: Op\u00e7\u00f5es de relacionamento com avan\u00e7o autom\u00e1tico\r\n            const optionButtons = document.querySelectorAll(\"#step-2 .option-button\");\r\n            optionButtons.forEach(function(button) {\r\n                button.addEventListener(\"click\", function() {\r\n                    optionButtons.forEach(function(btn) {\r\n                        btn.classList.remove(\"selected\");\r\n                    });\r\n                    button.classList.add(\"selected\");\r\n                    quizData.relationship = button.dataset.value;\r\n                    autoAdvance();\r\n                });\r\n            });\r\n\r\n            \/\/ Etapa 12: Op\u00e7\u00f5es de personalidade com avan\u00e7o autom\u00e1tico\r\n            const personalityButtons = document.querySelectorAll(\"#step-12 .personality-button\");\r\n            personalityButtons.forEach(function(button) {\r\n                button.addEventListener(\"click\", function() {\r\n                    personalityButtons.forEach(function(btn) {\r\n                        btn.classList.remove(\"selected\");\r\n                    });\r\n                    button.classList.add(\"selected\");\r\n                    quizData.personality = button.dataset.personality;\r\n                    autoAdvance();\r\n                });\r\n            });\r\n\r\n            \/\/ Etapa 13: Op\u00e7\u00f5es de voz SEM avan\u00e7o autom\u00e1tico\r\n            const voiceButtons = document.querySelectorAll(\"#step-13 .voice-button\");\r\n            voiceButtons.forEach(function(button) {\r\n                button.addEventListener(\"click\", function() {\r\n                    if (currentAudio) {\r\n                        currentAudio.pause();\r\n                        currentAudio.currentTime = 0;\r\n                    }\r\n\r\n                    voiceButtons.forEach(function(btn) {\r\n                        btn.classList.remove(\"selected\");\r\n                    });\r\n                    button.classList.add(\"selected\");\r\n                    quizData.voice = button.dataset.voice;\r\n\r\n                    const audioSrc = button.dataset.audio;\r\n                    if (audioSrc) {\r\n                        currentAudio = new Audio(audioSrc);\r\n                        currentAudio.play();\r\n                    }\r\n\r\n                    \/\/ Habilitar bot\u00e3o pr\u00f3ximo na etapa de voz\r\n                    btnNext.disabled = false;\r\n                });\r\n            });\r\n\r\n            \/\/ Etapa 14: Sliders com bot\u00e3o pr\u00f3ximo manual\r\n            const sliderThumbs = document.querySelectorAll(\"#step-14 .slider-thumb\");\r\n            sliderThumbs.forEach(function(thumb) {\r\n                let isDragging = false;\r\n\r\n                thumb.addEventListener(\"mousedown\", function(e) {\r\n                    isDragging = true;\r\n                    e.preventDefault();\r\n                });\r\n\r\n                document.addEventListener(\"mousemove\", function(e) {\r\n                    if (isDragging) {\r\n                        const track = thumb.parentElement;\r\n                        const rect = track.getBoundingClientRect();\r\n                        const x = e.clientX - rect.left;\r\n                        const percentage = Math.max(0, Math.min(100, (x \/ rect.width) * 100));\r\n                        \r\n                        thumb.style.left = percentage + '%';\r\n                        \r\n                        const sliderName = thumb.dataset.slider;\r\n                        quizData[sliderName] = percentage;\r\n                    }\r\n                });\r\n\r\n                document.addEventListener(\"mouseup\", function() {\r\n                    isDragging = false;\r\n                });\r\n\r\n                \/\/ Touch events para mobile\r\n                thumb.addEventListener(\"touchstart\", function(e) {\r\n                    isDragging = true;\r\n                    e.preventDefault();\r\n                });\r\n\r\n                document.addEventListener(\"touchmove\", function(e) {\r\n                    if (isDragging) {\r\n                        const track = thumb.parentElement;\r\n                        const rect = track.getBoundingClientRect();\r\n                        const x = e.touches[0].clientX - rect.left;\r\n                        const percentage = Math.max(0, Math.min(100, (x \/ rect.width) * 100));\r\n                        \r\n                        thumb.style.left = percentage + '%';\r\n                        \r\n                        const sliderName = thumb.dataset.slider;\r\n                        quizData[sliderName] = percentage;\r\n                    }\r\n                });\r\n\r\n                document.addEventListener(\"touchend\", function() {\r\n                    isDragging = false;\r\n                });\r\n            });\r\n\r\n            \/\/ Etapa 15: Caracter\u00edsticas incomodas com avan\u00e7o autom\u00e1tico\r\n            const characteristicButtons = document.querySelectorAll(\"#step-15 .characteristic-button\");\r\n            characteristicButtons.forEach(function(button) {\r\n                button.addEventListener(\"click\", function() {\r\n                    characteristicButtons.forEach(function(btn) {\r\n                        btn.classList.remove(\"selected\");\r\n                    });\r\n                    button.classList.add(\"selected\");\r\n                    quizData.characteristic = button.dataset.characteristic;\r\n                    autoAdvance();\r\n                });\r\n            });\r\n\r\n            \/\/ Etapa 16: Sele\u00e7\u00e3o de fotos com avan\u00e7o autom\u00e1tico\r\n            const photoOptions = document.querySelectorAll(\"#step-16 .photo-option\");\r\n            photoOptions.forEach(function(button) {\r\n                button.addEventListener(\"click\", function() {\r\n                    photoOptions.forEach(function(btn) {\r\n                        btn.classList.remove(\"selected\");\r\n                    });\r\n                    button.classList.add(\"selected\");\r\n                    quizData.photos = button.dataset.photos;\r\n                    autoAdvance();\r\n                });\r\n            });\r\n\r\n            \/\/ Etapa 17: Sele\u00e7\u00e3o de v\u00eddeos com avan\u00e7o autom\u00e1tico\r\n            const videoOptions = document.querySelectorAll(\"#step-17 .video-option\");\r\n            videoOptions.forEach(function(button) {\r\n                button.addEventListener(\"click\", function() {\r\n                    videoOptions.forEach(function(btn) {\r\n                        btn.classList.remove(\"selected\");\r\n                    });\r\n                    button.classList.add(\"selected\");\r\n                    quizData.videos = button.dataset.videos;\r\n                    autoAdvance();\r\n                });\r\n            });\r\n\r\n            \/\/ Navega\u00e7\u00e3o manual (para etapas 13 e 14)\r\n            btnNext.addEventListener(\"click\", function() {\r\n                if (currentStep === 13 || currentStep === 14) {\r\n                    autoAdvance();\r\n                }\r\n            });\r\n\r\n            btnBack.addEventListener(\"click\", function() {\r\n                if (currentStep > 1) {\r\n                    currentStep--;\r\n                    updateUI();\r\n                }\r\n            });\r\n\r\n            \/\/ Inicializar valores padr\u00e3o dos sliders\r\n            quizData['timida-safada'] = 50;\r\n            quizData['seria-engracada'] = 50;\r\n            quizData['logica-emocional'] = 50;\r\n            quizData['fria-carinhosa'] = 50;\r\n\r\n            \/\/ Inicializar\r\n            updateUI();\r\n        });\r\n    <\/script>\r\n<\/body>\r\n<\/html>\r\n\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>Quiz IA &#8211; Completo Qual o estilo da sua IA? Realista Anime Fantasia Homem Amiga Confidente Amante Namorada Escrava Sexual Perseguidora Obcecada Tarada Insaci\u00e1vel 18-24 25-34 35-49 +50 Magra Fitness Atl\u00e9tica Plus Size Pequenos M\u00e9dios Grandes Enormes Pequena M\u00e9dia Grande Enorme Asi\u00e1tica Latina Europeia Afro Castanhos Azuis Verdes Mel Cinza Pretos Vermelhos Roxos \u00c2mbar Liso [&hellip;]<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"elementor_canvas","meta":{"footnotes":""},"class_list":["post-9","page","type-page","status-publish","hentry"],"_links":{"self":[{"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/pages\/9","targetHints":{"allow":["GET"]}}],"collection":[{"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/pages"}],"about":[{"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/types\/page"}],"author":[{"embeddable":true,"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/comments?post=9"}],"version-history":[{"count":58,"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/pages\/9\/revisions"}],"predecessor-version":[{"id":155,"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/pages\/9\/revisions\/155"}],"wp:attachment":[{"href":"https:\/\/namor-ia.com\/wp-json\/wp\/v2\/media?parent=9"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}